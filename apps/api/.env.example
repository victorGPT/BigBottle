PORT=4000
CORS_ORIGIN=http://localhost:5173

# Used to sign API access tokens (JWT)
JWT_SECRET=change-me-change-me

# Supabase (DB)
SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=

# AWS
AWS_REGION=
S3_BUCKET=
# If you are not using an IAM role / instance profile, set static credentials for local dev:
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
# AWS_SESSION_TOKEN=

# Dify
# - mock: return a fixture response (local dev / tests)
# - workflow: call Dify Workflow API
DIFY_MODE=mock
# Dify workflow image input is configured as a file input.
# The API passes a remote URL payload:
#   { type: "image", transfer_method: "remote_url", url: "<presigned-get-url>" }
DIFY_API_URL=
DIFY_API_KEY=
DIFY_WORKFLOW_ID=
DIFY_IMAGE_INPUT_KEY=image_url
# Dify call timeout in milliseconds
DIFY_TIMEOUT_MS=20000

# Presign TTL in seconds
S3_PRESIGN_EXPIRES_SECONDS=300

# --- Phase 2 (Rewards / On-chain B3TR claim) ---
# Network: testnet or mainnet
VECHAIN_NETWORK=testnet
# Optional override. Defaults:
# - testnet: https://testnet.vechain.org
# - mainnet: https://mainnet.vechain.org
VECHAIN_NODE_URL=

# VeBetterDAO app id (bytes32 hex, 0x...)
VEBETTER_APP_ID=
# X2EarnRewardsPool address (per network).
X2EARN_REWARDS_POOL_ADDRESS=

# Fee delegation (VIP-191) sponsor URL (vechain.energy or your own gas payer service).
FEE_DELEGATION_URL=

# Reward distributor private key (0x...) for X2EarnRewardsPool distribution calls.
REWARD_DISTRIBUTOR_PRIVATE_KEY=
